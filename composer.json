{
    "name": "f4lk0n/core-dev",
    "description": "F4lk0n Var Dump",
    "type": "library",
    "keywords": [
        "debug",
        "dump"
    ],
    "homepage": "https://github.com/FalkonCorp/core-dev",
    "license": "MIT",
    "authors": [
        {
            "name": "F4lk0n",
            "email": "otaviosoria@gmail.com",
            "homepage": "https://github.com/F4LK0N",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "F4lk0n\\CoreDev\\": "src/"
        },
        "files": [
            "src/VDD.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "scripts": {
        "da": "composer dumpautoload",
        "dao": "composer dumpautoload --classmap-authoritative --optimize",

        "test":  "vendor/bin/phpunit --no-coverage",
        "dox":   "vendor/bin/phpunit --no-coverage --testdox",
        "cover": "vendor/bin/phpunit"
    }
}
